{
  "comment": "This generates one of the curves in a plot illustrating the bad",
  "comment": "effects of dealing with a competing workload with smaller blocks",
  "comment": "that can cut ahead in the contest for CUs.",
  "name": "Effects of blocks 'cutting ahead'",
  "max_iterations": 1000000,
  "max_time": 5,
  "sync_every_iteration": false,
  "gpu_device_id": 0,
  "pin_cpus": true,
  "omit_block_times": true,
  "use_processes": true,
  "do_warmup": true,
  "plugins": [
    {
      "label": "vs. 256-thread competing blocks",
      "log_name": "results/cutting_ahead_vs_256.json",
      "filename": "./bin/matrix_multiply.so",
      "thread_count": 1024,
      "block_count": 1,
      "additional_info": {
        "matrix_width": 1024,
        "skip_copy": true
      }
    },
    {
      "label": "competitor",
      "log_name": "/dev/null",
      "filename": "./bin/matrix_multiply.so",
      "thread_count": 256,
      "block_count": 1,
      "additional_info": {
        "matrix_width": 1024,
        "skip_copy": true
      }
    }
  ]
}
