{
  "name": "Concurrent GPU Threads",
  "max_iterations": 1,
  "max_time": 0,
  "gpu_device_id": 1,
  "use_processes": true,
  "do_warmup": true,
  "plugins": [
    {
      "label": "Backprop, all CUs",
      "comment": "The backprop benchmark ignores thread and block count settings.",
      "filename": "./bin/backprop.so",
      "log_name": "results/timeline_figure_backprop.json",
      "thread_count": 0,
      "block_count": 0
    },
    {
      "label": "DWT2D, single CU",
      "comment": "The dwt benchmark ignores thread and block count settings.",
      "filename": "./bin/dwt2d.so",
      "log_name": "results/timeline_figure_dwt2d.json",
      "thread_count": 0,
      "block_count": 0,
      "compute_unit_mask": "0x80000000"
    }
  ]
}
